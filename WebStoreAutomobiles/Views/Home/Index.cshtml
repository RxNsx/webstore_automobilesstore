@model ProductsListViewModels
@{
    ViewData["Title"] = "Home Page";
}

@foreach(Product p in Model.Products)
{
    <div>
        <p>Name: @p.Name</p>
        <p>Description: @p.Description</p>
        <p>Price: @p.Price</p>
        <p>Category: @p.Category</p>
    </div>
}

<div page-model="@Model.PagingInfo" page-action="Index"></div>